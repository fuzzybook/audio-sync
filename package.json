{
  "name": "rollup-experiment",
  "version": "1.0.0",
  "description": "RollUp experiment",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuzzybook/rollup-experiment.git"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp && terser --compress --mangle --mangle-props -- dist/js/library.js > dist/js/library.min.js",
    "lint": "eslint ./src/**/*.js --ext js",
    "clean": "rm -rf ./dist && mkdir dist",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "test": "jest --watchAll",
    "test:build": "jest",
    "jest:coverage": "jest --coverage",
    "production": "cp -r dist/ /Users/fabio/CODE/SQUYNC/squync-frontend/src/statics/assets/editor/."
  },
  "keywords": [
    "RollUp"
  ],
  "author": "Fuzzybook",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fuzzybook/rollup-experiment/issues"
  },
  "homepage": "https://github.com/fuzzybook/rollup-experiment#readme",
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babelrc-rollup": "^3.0.0",
    "browser-sync": "^2.26.7",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "event-emitter": "^0.3.5",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-babel": "8.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "6.2.0",
    "gulp-minify-css": "1.2.4",
    "gulp-plumber": "1.2.1",
    "gulp-sass": "4.0.2",
    "gulp-uglify": "3.0.2",
    "jest": "^24.9.0",
    "jest-mock-console": "^1.0.0",
    "pump": "3.0.0",
    "rollup": "^1.27.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-browsersync": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "terser": "^5.7.0"
  },
  "dependencies": {
    "gulp-sourcemaps": "^2.6.5",
    "hyperhtml": "^2.31.2",
    "node-sass": "^4.13.0",
    "recorder-js": "^1.0.7",
    "rollup-plugin-uglify": "^6.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "modulePaths": [
      "src"
    ]
  }
}
